
@{
    ViewBag.Title = "List";
}

<h1>List</h1>
<div>
    <table class="table table-bordered" >
        <tr>
            <th>ID</th>
            <th>Name</th>
        </tr>
        @foreach(var p in Model){
        <tr>
            <td>@p.Id</td>
            <td>@p.Name</td>
        </tr> 
        }
        
    </table>
</div>

